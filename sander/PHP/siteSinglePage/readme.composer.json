// Тут конфиг для composer. 
// Он должен называться "composer.json", но т.к. это просто пример, то к названию добавлено "readme."
// composer затянет необходимые зависимости в vendor/.
// Чтобы попробовать, надо: удалить шапку с текстом; переименовать "readme.composer.json"; выполнить из консоли команду "composer update".
// Выглядит он подобным образом:
{
    "name": "zendframework/skeleton-application",
    "description": "Skeleton Application for ZF2",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "zf2"
    ],
    "homepage": "http://framework.zend.com/",
    "require": {
        "php": ">=5.5",
        "zendframework/zendframework": "~2.5"
    }
}
